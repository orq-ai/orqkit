{
  "name": "@orq-ai/mcp-server",
  "version": "0.1.0",
  "description": "MCP server for Orq AI platform - prompts and datasets",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "orq-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "bunx tsx src/index.ts",
    "dev:sse": "bunx tsx src/index.ts --sse",
    "inspect": "bunx @modelcontextprotocol/inspector",
    "start": "bun dist/index.js",
    "start:sse": "bun dist/index.js --sse",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@orq-ai/node": "^3.14.45",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.18.1"
  },
  "keywords": [
    "mcp",
    "orq",
    "ai",
    "prompts",
    "datasets"
  ],
  "license": "MIT"
}
