{
  "name": "n8n-nodes-orq",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/n8n-nodes-orq/src",
  "projectType": "library",
  "targets": {
    "build": {
      "command": "bunx rimraf dist && bunx tsc -p tsconfig.lib.json && bun load-icons.ts",
      "options": {
        "cwd": "packages/n8n-nodes-orq"
      }
    },
    "dev": {
      "command": "tsc --watch",
      "options": {
        "cwd": "packages/n8n-nodes-orq"
      }
    },
    "test:local": {
      "command": "bun link && n8n start",
      "options": {
        "cwd": "packages/n8n-nodes-orq"
      },
      "dependsOn": ["build"]
    },
    "test:local:custom": {
      "command": "N8N_CUSTOM_EXTENSIONS=\"$(pwd)/packages/n8n-nodes-orq\" n8n start",
      "dependsOn": ["build"]
    }
  },
  "tags": []
}
