{
  "name": "n8n-nodes-orq",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/n8n-nodes-orq/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/n8n-nodes-orq/dist",
        "main": "packages/n8n-nodes-orq/src/index.ts",
        "tsConfig": "packages/n8n-nodes-orq/tsconfig.lib.json",
        "assets": [
          {
            "input": "packages/n8n-nodes-orq/src",
            "glob": "**/*.{svg,png,json}",
            "output": "src"
          }
        ]
      }
    },
    "typecheck": {
      "executor": "@nx/js:tsc",
      "options": {
        "tsConfig": "packages/n8n-nodes-orq/tsconfig.lib.json",
        "noEmit": true
      }
    },
    "dev": {
      "command": "tsc --watch",
      "options": {
        "cwd": "packages/n8n-nodes-orq"
      }
    },
    "test:local": {
      "command": "npm link && n8n start",
      "options": {
        "cwd": "packages/n8n-nodes-orq"
      },
      "dependsOn": ["build"]
    },
    "test:local:custom": {
      "command": "N8N_CUSTOM_EXTENSIONS=\"$(pwd)/packages/n8n-nodes-orq\" n8n start",
      "dependsOn": ["build"]
    }
  },
  "tags": []
}