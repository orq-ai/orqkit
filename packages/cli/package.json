{
  "name": "@orq-ai/cli",
  "version": "1.0.9",
  "description": "Command-line interface for discovering and running evaluation files with @orq-ai/evaluatorq, and working with Orq AI platform",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "cli",
    "ai",
    "evaluation",
    "testing",
    "orq",
