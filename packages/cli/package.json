      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "tslib": "^2.8.0",
    "commander": "^12.0.0",
    "glob": "^10.3.10",
    "tsx": "^4.7.0",
